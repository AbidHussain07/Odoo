<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<!-- noupdate means that the data will not be updated if the module is updated or changed. -->
<!-- force_create="1" means that the data will be created forcefully. --> 
<!-- force_create="0" means that the data will be not created next time forcefully. if it is deleted by the user --> 

    <data noupdate="1">
        <record id="patient_tag_1" model="patient.tag" forcecreate="0">
            <field name="name">Fever</field>
        </record>
        <record id="patient_tag_2" model="patient.tag" forcecreate="0">
            <field name="name">Cancer</field>
        </record>
        <record id="patient_tag_3" model="patient.tag" forcecreate="0" >
            <field name="name">Psychology</field>
        </record>
        <record id="patient_tag_4" model="patient.tag" forcecreate="0">
            <field name="name">Depression</field>
        </record>
        <record id="patient_tag_5" model="patient.tag" forcecreate="1">
            <field name="name">Mansik</field>
        </record>
    </data>
</odoo>
