<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1"> <!-- noupdate means if we change in UI interface it will accept that only even after update -->

    <!-- Sequences for hospital appointment -->
    <record id="seq_appointment" model="ir.sequence">
        <field name="name">Reference No.</field>
        <field name="code">hospital.appointment</field>
        <field name="prefix">HOS</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</data>


</odoo>